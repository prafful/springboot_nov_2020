<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="crm" match-schema="INFORMATION_SCHEMA" match-name=".*"/>
  <table-filter match-catalog="crm" match-schema="production" match-name=".*"/>
  <table-filter match-catalog="crm" match-schema="sales" match-name=".*"/>
  <table-filter match-catalog="crm" match-schema="sys" match-name=".*"/>
</hibernate-reverse-engineering>